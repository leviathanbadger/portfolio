{
    "version": "0.1.0",
    "configurations": [
        {
            "name": "Launch project (Firefox)",
            "type": "firefox",
            "request": "launch",
            "reAttach": true,
            "url": "http://localhost:4200/",
            "webRoot": "${workspaceFolder}"
        },
        {
            "name": "Launch project (Chrome)",
            "type": "chrome",
            "request": "launch",
            "sourceMaps": true,
            "smartStep": true,
            "url": "http://localhost:4200/"
        }
    ]
}
